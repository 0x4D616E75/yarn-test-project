{
  "name": "test-project",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.10.3",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -A run build",
    "dev": "yarn workspace @test-project/api run dev",
    "start": "yarn workspace @test-project/api run start",
    "test": "yarn workspaces foreach -A run test",
    "clean": "yarn workspaces foreach -A run clean"
  },
  "devDependencies": {
    "@swc/core": "1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
